{
    "name": "alkemi-ui-template",
    "version": "0.1.1",
    "description": "Front End Template with React, Redux, Docker, Web3",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:project-alkemi/alkemi-ui-template"
    },
    "keywords": [
        "react",
        "alkemi"
    ],
    "author": "Alkemi",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/project-alkemi/alkemi-ui-template/issues"
    },
    "dependencies": {
        "@material-ui/core": "^3.1.0",
        "@material-ui/icons": "^3.0.0",
        "connected-react-router": "^6.2.2",
        "history": "^4.7.2",
        "material-ui": "^0.20.2",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-redux": "^6.0.0",
        "react-router": "^4.3.0",
        "react-scripts": "^2.0.0",
        "redux": "^4.0.1",
        "redux-saga": "^1.1.3",
        "reselect": "^4.0.0",
        "sass": "^1.26.3",
        "styled-components": "^5.0.1",
        "web3-react": "^5.0.5",
        "web3modal": "^1.1.0"
    },
    "devDependencies": {
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "enzyme-to-json": "^3.3.0",
        "husky": "^1.1.0",
        "lint-staged": "^10.5.3",
        "prettier": "^1.10.2",
        "redux-test-utils": "^0.3.0"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,css}": [
            "prettier --write",
            "git add"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!**/src/index.js",
            "!**/src/registerServiceWorker.js"
        ]
    },
    "prettier": {
        "useTabs": false,
        "tabWidth": 4
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
